{
  "schemaVersion": 1,
  "id": "${mod_id}-${minecraft_version_id}",
  "version": "${mod_version}",
  "icon":,
  "name": "${mod_name} for ${minecraft_version}",
  "description": "${mod_name} for ${minecraft_version}",
  "authors": [
    {
      "name": "wendavid552",
      "contact": {
        "homepage": "https://github.com/wendavid552"
      }
    }
  ],
  "environment": "client",
  "entrypoints": {
    "client": [
      "net.arcueid.betterRedstoneMusicClient.BetterMusicClient"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "minecraft": "${minecraft_dependency}",
    "magiclib-${minecraft_version_id}": ">=${magiclib_version}"
  },
  "custom": {
    "modmenu:clientsideOnly": true,
    "modmenu:parent": "${mod_id}",
    "modmenu": {
      "parent": "${mod_id}"
    }
  }
}
